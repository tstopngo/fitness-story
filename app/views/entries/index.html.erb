<h1>All Entries</h1>
<% @entries.each do |entry|%>
  <div id="<%= entry.id%>" class="entry">
    <ul>
      <a href="/users/<%=entry.user.username%>"><li> By:  <%= entry.user.username %></li></a>
      <li> Workout Calories: <%= entry.workout_calories %> </li>
      <li> Food Calories: <%= entry.food_calories %></li>
      <li> Net Calories: <%= entry.net_calories %> </li>
      <li> Food: <%= entry.food %> </li>
    </ul>
  </div>
  <br>
  <br>
<%end%>
